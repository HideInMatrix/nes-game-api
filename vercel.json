{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/schema.prisma", "prisma/**/*.prisma"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/main.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "HEAD"]
    }
  ]
}
